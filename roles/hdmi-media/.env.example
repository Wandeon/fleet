MEDIA_CONTROL_TOKEN=changeme-token
HDMI_CONNECTOR=HDMI-A-1
HDMI_AUDIO_DEVICE=plughw:vc4hdmi,0
CEC_DEVICE_INDEX=0
CEC_OSD_NAME=%H
# Use a stable by-id path from "ls -l /dev/serial/by-id".
ZIGBEE_SERIAL_PORT=/dev/serial/by-id/usb-dresden_elektronik_ConBee_III_DEXXXXXXXX-if00-port0
# Credentials must match the broker password file generated by the agent.
ZIGBEE_MQTT_USER=zigbee
ZIGBEE_MQTT_PASSWORD=zigbee-password
ZIGBEE_NETWORK_KEY=GENERATE
ZIGBEE_PAN_ID=0x1A62
ZIGBEE_EXT_PAN_ID=0xdddddddddddddddd
ZIGBEE_CHANNEL=11
ZIGBEE_FRONTEND_PORT=8084
ZIGBEE_PERMIT_JOIN=false
